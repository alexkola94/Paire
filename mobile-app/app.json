{
  "expo": {
    "name": "Paire",
    "slug": "paire",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "paire",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8e44ad"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paire.expenses",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8e44ad"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.paire.expenses",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Paire to access your photos for receipts and attachments."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f259dae3-0cfe-4feb-8201-baa563e067fb"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f259dae3-0cfe-4feb-8201-baa563e067fb"
    }
  }
}
