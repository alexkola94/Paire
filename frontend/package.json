{
  "name": "paire-frontend",
  "version": "2.1.1",
  "description": "Paire - Expense tracking app for couples",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <25.0.0",
    "npm": ">=9.0.0"
  },
  "homepage": "https://alexkola94.github.io/Paire",
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js",
    "build": "node node_modules/vite/bin/vite.js build",
    "preview": "node node_modules/vite/bin/vite.js preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "node node_modules/vitest/vitest.mjs",
    "test:ui": "node node_modules/vitest/vitest.mjs --ui",
    "test:coverage": "node node_modules/vitest/vitest.mjs --coverage",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,json}\""
  },
  "dependencies": {
    "@microsoft/signalr": "^10.0.0",
    "@tanstack/react-query": "^5.90.12",
    "chart.js": "^4.5.1",
    "date-fns": "^3.0.6",
    "dexie": "^4.2.1",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.26.2",
    "i18next": "^23.7.16",
    "mapbox-gl": "^3.18.0",
    "pdfjs-dist": "^5.4.530",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-icons": "^5.0.1",
    "react-map-gl": "^8.1.0",
    "react-markdown": "^10.1.0",
    "react-otp-input": "^3.1.1",
    "react-plaid-link": "^4.1.1",
    "react-router-dom": "^6.21.1",
    "react-window": "^2.2.5",
    "recharts": "^2.15.4",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "vercel": "^25.2.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.28.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16"
  }
}
