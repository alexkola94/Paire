    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.15.4"
